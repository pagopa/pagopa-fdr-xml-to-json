{
  "version": "2.0",
  "extensionBundle": {
    "id": "Microsoft.Azure.Functions.ExtensionBundle",
    "version": "[4.0.0, 5.0.0)"
  },
  "extensions": {
    "tracing": {
      "traceInputsAndOutputs": false,
      "traceReplayEvents": false
    },
    "http": {
      "routePrefix": ""
    },
    "blobs": {
      "maxDegreeOfParallelism": 2,
      "poisonBlobThreshold": 1
    },
    "queues": {
      "batchSize": 1,
      "maxDequeueCount": 4,
      "visibilityTimeout" : "00:00:30"

    }
  },
  "logging": {
    "logLevel": {
      "Function.BlobEventProcessor": "Error",
      "Function.QueueEventProcessor": "Error",
      "Function.XmlErrorRetry": "Error",
      "Function.HttpTriggerFn": "Error",
      "Function.ErrorRecoveryFn": "Error",
      "Function.Info": "Error"
    },
    "applicationInsights": {
      "samplingSettings": {
        "isEnabled": true,
        "maxTelemetryItemsPerSecond": 5,
        "includedTypes": "PageView;Trace;Dependency;Request",
        "excludedTypes": "Exception;Event;CustomEvent"
      }
    }
  }
}
