{
  "openapi": "3.0.1",
  "info": {
    "title": "Azure Functions API",
    "version": "1.0.1-5-PAGOPA-2470-fix",
    "description": "API specification for Azure Functions XmlErrorRetry and Info"
  },
  "paths": {
    "/xmlerror": {
      "get": {
        "summary": "Retry XML Error Processing",
        "operationId": "XmlErrorRetry",
        "parameters": [
          {
            "name": "partitionKey",
            "in": "query",
            "required": false,
            "schema": {
              "type": "string"
            },
            "description": "Partition key to identify the entity"
          },
          {
            "name": "rowKey",
            "in": "query",
            "required": false,
            "schema": {
              "type": "string"
            },
            "description": "Row key to identify the entity"
          },
          {
            "name": "deleteOnlyByKey",
            "in": "query",
            "required": false,
            "schema": {
              "type": "boolean"
            },
            "description": "Flag to delete only by key"
          }
        ],
        "responses": {
          "200": {
            "description": "Success message",
            "content": {
              "text/plain": {
                "schema": {
                  "type": "string"
                }
              }
            }
          },
          "500": {
            "description": "Internal Server Error",
            "content": {
              "text/plain": {
                "schema": {
                  "type": "string"
                }
              }
            }
          }
        }
      }
    },
    "/info": {
      "get": {
        "summary": "Get API Info",
        "operationId": "Info",
        "responses": {
          "200": {
            "description": "Success",
            "content": {
              "text/plain": {
                "schema": {
                  "type": "string"
                }
              }
            }
          }
        }
      }
    }
  }
}
