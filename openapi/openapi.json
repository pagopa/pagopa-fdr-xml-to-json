{
  "openapi": "3.0.1",
  "info": {
    "title": "FdR - XML to JSON ${service}",
    "version": "1.0.4",
    "description": "FDR XML to JSON API REST"
  },
  "paths": {
    "/xmlerror": {
      "get": {
        "summary": "Retry XML Error Processing",
        "operationId": "XmlErrorRetry",
        "parameters": [
          {
            "name": "partitionKey",
            "in": "query",
            "required": false,
            "schema": {
              "type": "string"
            },
            "description": "Date in format 'yyyy-mm-dd' - table fdr1conversionerror"
          },
          {
            "name": "rowKey",
            "in": "query",
            "required": false,
            "schema": {
              "type": "string"
            },
            "description": "SessionId (blob metadata) - table fdr1conversionerror"
          }
        ],
        "responses": {
          "200": {
            "description": "Success message",
            "content": {
              "text/plain": {
                "schema": {
                  "type": "string"
                }
              }
            }
          },
          "500": {
            "description": "Internal Server Error",
            "content": {
              "text/plain": {
                "schema": {
                  "type": "string"
                }
              }
            }
          }
        }
      }
    },
    "/fdr": {
      "get": {
        "summary": "Retry to processing filename",
        "operationId": "HttpTriggerFn",
        "parameters": [
          {
            "name": "filename",
            "in": "query",
            "required": false,
            "schema": {
              "type": "string"
            },
            "description": "Blob filename to recover"
          }
        ],
        "responses": {
          "200": {
            "description": "Success message",
            "content": {
              "text/plain": {
                "schema": {
                  "type": "string"
                }
              }
            }
          },
          "500": {
            "description": "Internal Server Error",
            "content": {
              "text/plain": {
                "schema": {
                  "type": "string"
                }
              }
            }
          }
        }
      }
    },
    "/info": {
      "get": {
        "summary": "Get API Info",
        "operationId": "Info",
        "responses": {
          "200": {
            "description": "Success",
            "content": {
              "text/plain": {
                "schema": {
                  "type": "string"
                }
              }
            }
          }
        }
      }
    }
  }
}
